    <script>
      (function($) {
        $.scrollToElement = function($element, speed) {

            speed = speed || 750;

            $("html, body").animate({
                scrollTop: $element.offset().top,
                scrollLeft: $element.offset().left
            }, speed);
            return $element;
        };

        $.fn.scrollTo = function(speed) {
            speed = speed || "normal";
            return $.scrollToElement(this, speed);
        };
    })(jQuery);

